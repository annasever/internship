{
    "dashboard": {
        "id": null,
        "uid": "redis_dashboard",
        "title": "Redis Dashboard",
        "tags": [],
        "timezone": "browser",
        "schemaVersion": 26,
        "version": 1,
        "panels": [
            {
                "type": "graph",
                "title": "Commands Per Second",
                "targets": [
                    {
                        "target": "sum(rate(redis_commands_processed[1m]))",
                        "refId": "A"
                    }
                ],
                "xaxis": {
                    "mode": "time"
                },
                "yaxes": [
                    {
                        "format": "short",
                        "label": "Commands",
                        "logBase": 1,
                        "min": 0,
                        "max": null
                    },
                    {
                        "format": "short",
                        "label": null,
                        "logBase": 1,
                        "min": 0,
                        "max": null
                    }
                ]
            },
            {
                "type": "graph",
                "title": "Memory Usage",
                "targets": [
                    {
                        "target": "redis_memory_used_bytes / 1024 / 1024",
                        "refId": "A"
                    }
                ],
                "xaxis": {
                    "mode": "time"
                },
                "yaxes": [
                    {
                        "format": "MB",
                        "label": "Memory (MB)",
                        "logBase": 1,
                        "min": 0,
                        "max": null
                    },
                    {
                        "format": "short",
                        "label": null,
                        "logBase": 1,
                        "min": 0,
                        "max": null
                    }
                ]
            },
            {
                "type": "graph",
                "title": "Keyspace Hits and Misses",
                "targets": [
                    {
                        "target": "sum(rate(redis_keyspace_hits[1m]))",
                        "refId": "A"
                    },
                    {
                        "target": "sum(rate(redis_keyspace_misses[1m]))",
                        "refId": "B"
                    }
                ],
                "xaxis": {
                    "mode": "time"
                },
                "yaxes": [
                    {
                        "format": "short",
                        "label": "Keys",
                        "logBase": 1,
                        "min": 0,
                        "max": null
                    },
                    {
                        "format": "short",
                        "label": null,
                        "logBase": 1,
                        "min": 0,
                        "max": null
                    }
                ],
                "seriesOverrides": [
                    {
                        "alias": "Hits",
                        "color": "green"
                    },
                    {
                        "alias": "Misses",
                        "color": "red"
                    }
                ]
            }
        ]
    }
}