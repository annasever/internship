{
    "dashboard": {
        "id": null,
        "uid": "frontend_dashboard",
        "title": "Frontend Dashboard",
        "tags": [],
        "timezone": "browser",
        "schemaVersion": 26,
        "version": 1,
        "panels": [
            {
                "type": "graph",
                "title": "HTTP Requests",
                "targets": [
                    {
                        "target": "sum(rate(http_requests_total[1m]))",
                        "refId": "A"
                    }
                ],
                "xaxis": {
                    "mode": "time"
                },
                "yaxes": [
                    {
                        "format": "short",
                        "label": "Requests",
                        "logBase": 1,
                        "min": 0,
                        "max": null
                    },
                    {
                        "format": "short",
                        "label": null,
                        "logBase": 1,
                        "min": 0,
                        "max": null
                    }
                ]
            },
            {
                "type": "graph",
                "title": "Response Time",
                "targets": [
                    {
                        "target": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
                        "refId": "A"
                    }
                ],
                "xaxis": {
                    "mode": "time"
                },
                "yaxes": [
                    {
                        "format": "ms",
                        "label": "Time (ms)",
                        "logBase": 1,
                        "min": 0,
                        "max": null
                    },
                    {
                        "format": "short",
                        "label": null,
                        "logBase": 1,
                        "min": 0,
                        "max": null
                    }
                ]
            },
            {
                "type": "singlestat",
                "title": "Error Rate",
                "targets": [
                    {
                        "target": "sum(rate(http_requests_total{status!=\"200\"}[1m])) / sum(rate(http_requests_total[1m])) * 100",
                        "refId": "A"
                    }
                ],
                "valueName": "current",
                "prefix": "%",
                "thresholds": "0,1",
                "colorBackground": true,
                "colors": [
                    "green",
                    "red"
                ]
            }
        ]
    }
}